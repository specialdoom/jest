{
  "name": "@storybook/jest",
  "version": "0.1.0",
  "description": "Instrumented version of Jest for Storybook Interactions",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/jest.git"
  },
  "license": "MIT",
  "author": "yannbf@gmail.com",
  "main": "./dist/index.js",
  "module": "./dist/index.,js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "prerelease": "yarn build",
    "release": "auto shipit"
  },
  "dependencies": {
    "@storybook/expect": "storybook-jest",
    "@testing-library/jest-dom": "^5.16.2",
    "jest-mock": "^27.3.0"
  },
  "devDependencies": {
    "@auto-it/first-time-contributor": "^10.37.6",
    "@auto-it/released": "^10.37.6",
    "@storybook/instrumenter": "future",
    "@storybook/linter-config": "^3.1.2",
    "@types/react": "*",
    "auto": "^10.37.6",
    "expect": "^27.3.1",
    "tsup": "^5.12.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "auto": {
    "prereleaseBranches": [
      "next",
      "prerelease",
      "future"
    ],
    "plugins": [
      "npm",
      "first-time-contributor",
      "released"
    ]
  }
}
